{
  "expo": {
    "name": "faceit-tracker",
    "slug": "faceit-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/app-icon.png",
      "backgroundColor": "#262626",
      "max-height": "100%",
      "max-width": "100%"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rike2000.faceittracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#262626"
      },
      "package": "com.rike2000.faceittracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "73fae057-c05c-402a-9c18-824d9c65e4ea"
      }
    }
  }
}
